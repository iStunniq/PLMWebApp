        <div class="col-6">
            <h2 class="text-primary">Inventory Products</h2>
        </div>
    </div>

    <br /> 

    <table id="tblData" class="table table-hover" style="width: 100%" > 
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Stock
                </th>
                <th>
                    Status
                </th>
                <th>
                    Price
                </th>
                <th>
                    Brand
                </th>
                <th>
                    Category
                </th>
                <th>

                </th>
            </tr>
        </thead>
    </table>

</div>


@section Scripts{
    <script src="~/js/inventory.js"></script>
}