@{
    ViewData["Title"] = "All Users";
}

<div class="container p-3">

    <div class="row pt-4"> 
        <div class="col-6">
            <h2 class="text-primary">All Users</h2>
        </div>
        <div class="col-6 text-end">
        </div>
    </div>

    <br /> 

    <table id="tblData" class="table table-hover" style="width: 100%" > 
        <thead>
            <tr>
                <th>
                    Email
                </th>
                <th>
                    First Name
                </th>
                <th>
                    Last Name
                </th>
                <th>
                    Phone
                </th>
                <th>
                    Role
                </th>
                <th>
                    Warnings
                </th>
                <th></th>
            </tr>
        </thead>
    </table>

</div>


@section Scripts{
    <script src="~/js/user.js"></script>
}